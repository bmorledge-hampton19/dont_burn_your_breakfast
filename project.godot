; Engine configuration file.
; It's best edited using the editor UI and not directly,
; since the parameters that go here are not all obvious.
;
; Format:
;   [section] ; section goes between []
;   param=value ; assign values to parameters

config_version=5

[application]

config/name="Don't Burn Your Breakfast"
config/description="A silly little game about the inherent dangers of the most important meal of the day.
Inspired by the timeless classic, \"Don't Shit Your Pants\"."
config/version="0.1.1"
run/main_scene="res://scenes/main_scenes/main_menu.tscn"
config/features=PackedStringArray("4.2", "C#", "Forward Plus")
boot_splash/show_image=false
config/icon="res://sprites/misc/app_icon.png"
config/windows_native_icon="res://sprites/misc/app_icon.ico"

[autoload]

SceneManager="*res://scripts/singletons/SceneManager.gd"
Options="*res://scripts/singletons/Options.gd"
EndingsManager="*res://scripts/singletons/EndingsManager.gd"
Helpers="*res://scripts/singletons/Helpers.gd"

[debug]

gdscript/warnings/integer_division=0

[display]

window/size/viewport_width=720
window/size/viewport_height=540
window/size/initial_position_type=4
window/size/window_width_override=720
window/size/window_height_override=540
window/stretch/mode="viewport"
window/stretch/aspect="expand"

[dotnet]

project/assembly_name="dont_burn_your_breakfast"

[filesystem]

import/blender/enabled=false

[gui]

theme/custom_font="res://other_resources/kongtext.ttf"

[rendering]

textures/canvas_textures/default_texture_filter=0
